package google;

import core.googleService;

public class googleLoginManager implements googleService{

	@Override
	public void sendToVerifyMail(String email) {
		System.out.println(email + "  doğrulama mail'i gönderildi.");
		
	}

	@Override
	public boolean checkVerifyAccount(String email) {
		System.out.println(email + " kullanıcı hesabı doğrulandı");
		return true;
	}

}
